{
  "name": "conference-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backend:install": "pip install -r backend/requirements.txt",
    "backend:db": "docker compose -f docker-compose.postgres.yml up -d",
    "backend:stack:up": "docker compose -f docker-compose.backend.yml up -d --build",
    "backend:stack:down": "docker compose -f docker-compose.backend.yml down",
    "stack:up": "docker compose up -d --build",
    "stack:down": "docker compose down",
    "stack:logs": "docker compose logs -f --tail=200",
    "dev": "bash -c 'python3 backend/run.py & WS_PID=$!; trap \"kill $WS_PID\" EXIT; next dev'",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "ws-server": "python3 backend/run.py",
    "ws-server:node": "node server/ws-server.js",
    "test:ws-load": "node scripts/ws-load-test.mjs"
  },
  "dependencies": {
    "@lottiefiles/dotlottie-react": "^0.14.4",
    "lucide-react": "^0.562.0",
    "next": "16.0.0",
    "ogl": "^1.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "unicode-emoji-json": "^0.8.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
